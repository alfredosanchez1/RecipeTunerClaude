{
  "name": "recipe-tuner",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "expo run:android --variant release",
    "build:android-dev": "expo run:android --variant debug",
    "build:ios": "expo run:ios --configuration Release",
    "build:ios-dev": "expo run:ios --configuration Debug",
    "prebuild": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "postbuild": "expo run:android --variant release",
    "build:apk": "npm run prebuild:android && npm run build:android",
    "build:apk-dev": "npm run prebuild:android && npm run build:android-dev",
    "build:ios-simulator": "npm run prebuild:ios && npm run build:ios-dev",
    "clean": "npx expo prebuild --clean",
    "install:apk": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "uninstall:app": "adb uninstall com.recipetuner.app",
    "reinstall:app": "npm run uninstall:app && npm run install:apk"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.3.17",
    "@stripe/stripe-js": "^7.9.0",
    "@stripe/stripe-react-native": "^0.54.0",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.5.0",
    "babel-preset-expo": "^54.0.1",
    "chalk": "^5.6.2",
    "expo": "~54.0.9",
    "expo-asset": "~12.0.8",
    "expo-camera": "~17.0.7",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.14",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.11",
    "expo-print": "~15.0.7",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.7",
    "invariant": "^2.2.4",
    "json5": "^2.2.3",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-calendar-events": "^2.2.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-mmkv": "^2.12.1",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-webview": "^13.16.0",
    "realm": "^20.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^20.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4"
  }
}
